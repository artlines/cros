<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Logs" table="logs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="entity" type="string" column="entity" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="event" type="text" column="event" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <field name="elementId" type="integer" column="element_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="readed" type="boolean" column="readed" nullable="false"/>
  </entity>
</doctrine-mapping>
