<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\OrgToConf" table="org_to_conf">
    <indexes>
      <index name="IDX_8062CCA932C8A3DE" columns="organization_id"/>
      <index name="IDX_8062CCA9604B8382" columns="conference_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="paid" type="integer" column="paid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="summ" type="string" column="summ" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paidSum" type="string" column="paid_sum" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="invoice" type="string" column="invoice" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paymentDate" type="datetime" column="payment_date" nullable="true"/>
    <many-to-one field="conference" target-entity="Conference" fetch="LAZY">
      <join-columns>
        <join-column name="conference_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="organization" target-entity="Organization" fetch="LAZY">
      <join-columns>
        <join-column name="organization_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
