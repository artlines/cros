<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Program" table="program">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="integer" column="date" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="start" type="string" column="start" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="end" type="string" column="end" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="conferenceId" type="integer" column="conference_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
